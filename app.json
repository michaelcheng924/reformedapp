{
  "expo": {
    "name": "ReformedApp",
    "slug": "learn-reformed",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.11",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.learnreformed",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "This app will not use this data. The data was only included because of the software used to build this app."
      }
    },
    "android": {
      "package": "com.catechismsandmore",
      "versionCode": 8
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
